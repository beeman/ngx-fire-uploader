{
  "name": "@ngx-fire-uploader/core",
  "description": "Angular file uploader for Firebase",
  "version": "0.9.8",
  "homepage": "http://github.com/murhafsousli/ngx-fire-uploader",
  "author": {
    "name": "Murhaf Sousli",
    "url": "https://github.com/murhafsousli"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/murhafsousli/ngx-fire-uploader.git"
  },
  "bugs": {
    "url": "https://github.com/murhafsousli/ngx-fire-uploader/issues"
  },
  "license": "MIT",
  "keywords": [
    "angular",
    "firebase",
    "file",
    "images",
    "upload",
    "uploader",
    "dropzone",
    "dropfile",
    "drop"
  ],
  "private": false,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "./node_modules/.bin/compodoc -p projects/core/tsconfig.lib.json -w -s",
    "sass-ts": "tsc .config/styles.ts --lib es7 && node .config/styles.js projects/manager/src/lib dist/manager",
    "sass": "node .config/styles.js projects/manager/src/lib dist/manager",
    "init-build": "npm run build-core && npm run version && npm run link-core && npm run build-manager && npm run version && npm run link-manager && npm run build-photo && npm run version && npm run link-photo && npm run sass-ts && npm run version",
    "build-all": "npm run build-core && npm run build-manager && npm run build-photo && npm run sass && npm run version",
    "build-core": "ng build core --prod",
    "build-manager": "ng build manager --prod",
    "build-photo": "ng build photo --prod",
    "link-core": "npm link dist/core && npm link @ngx-fire-uploader/core",
    "link-manager": "npm link dist/manager && npm link @ngx-fire-uploader/manager",
    "link-photo": "npm link dist/photo && npm link @ngx-fire-uploader/photo",
    "publish-all": "node .config/publish.js",
    "version": "node .config/version.js"
  },
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/common": "^6.1.2",
    "@angular/compiler": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/fire": "^5.1.1",
    "@angular/forms": "^6.1.2",
    "@angular/http": "^6.1.2",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@angular/router": "^6.1.2",
    "@firebase/app-types": "^0.3.2",
    "@ng-bootstrap/ng-bootstrap": "^2.2.2",
    "angular2-notifications": "^1.0.2",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "firebase": "^5.7.1",
    "rxjs": "^6.2.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.3",
    "@angular-devkit/build-ng-packagr": "~0.7.3",
    "@angular/cli": "^6.1.3",
    "@angular/compiler-cli": "^6.1.2",
    "@angular/language-service": "^6.1.2",
    "@compodoc/compodoc": "^1.1.3",
    "@types/bluebird": "^3.5.23",
    "@types/core-js": "^2.5.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.5.8",
    "ansi-colors": "^2.0.5",
    "autoprefixer": "^9.1.0",
    "codelyzer": "~4.4.3",
    "cssnano": "^4.0.5",
    "jasmine-core": "~3.2.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.2.0",
    "ng-packagr": "^3.0.6",
    "postcss": "^7.0.2",
    "postcss-strip-inline-comments": "^0.1.5",
    "protractor": "~5.4.0",
    "replace-ext": "^1.0.0",
    "ts-node": "~7.0.1",
    "tsickle": "0.34.0",
    "tslib": "^1.9.3",
    "tslint": "~5.11.0",
    "typescript": "^2.9.2"
  }
}
